{
  "name": "linkstash-nextjs",
  "description": "To save those links and always have them at hand.",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev-2": "cross-env NODE_NO_WARNINGS=1 next dev",
    "db:push": "npx drizzle-kit push",
    "db:studio": "npx drizzle-kit studio",
    "db:schema": "npx @better-auth/cli generate --config src/libs/auth.js"
  },
  "dependencies": {
    "@tailwindcss/postcss": "4.0.16",
    "better-auth": "1.2.4",
    "dotenv": "16.4.7",
    "drizzle-orm": "0.41.0",
    "next": "14.2.4",
    "postcss": "8.5.3",
    "react": "18",
    "react-dom": "18",
    "tailwindcss": "4.0.16"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "drizzle-kit": "0.30.5",
    "pg": "8.14.1",
    "standard": "17.1.2"
  },
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json"
  },
  "author": {
    "name": "Jes√∫s Rengifo Mendoza",
    "url": "https://github.com/JessEleven"
  },
  "license": "MIT",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
